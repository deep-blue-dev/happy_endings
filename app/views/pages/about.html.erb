<div class="about-location padding text-center">
  <h1>Location</h1>
  <!-- google -->
  <div class="maps location-map visible-md visible-lg">
    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3589.9153865752673!2d-80.21130398518845!3d25.872262110330244!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x88d9b195781dd15b%3A0x76f164762f0bfba3!2sHappy+Endings+Custom+Printed+Shirts+%26+Promotional+Items!5e0!3m2!1sen!2sus!4v1453966759464" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
  </div>
  <div class="address visible-sm visible-xs">
    <div class="row">
      <div class="col-xs-12 col-sm-12">
        <i class="fa fa-map-marker fa-2x"></i>
      </div>
      <div class="col-xs-12 col-sm-12">
        &nbsp; &nbsp; 651 NE 106 St, <br> &nbsp; &nbsp; Miami, FL 33150<br>
      </div>
    </div>
  </div>
  <div class="form-group form-wrapper">
    <h4>Get In Touch!</h4>
    <%= form_tag do %>
      <%= text_field_tag "name", nil, placeholder:"Name", class: "form-control" %><br>
      <%= text_field_tag "email", nil, placeholder:"Email", class: "form-control" %><br>
      <%= text_field_tag "phone", nil, placeholder:"Phone", class: "form-control" %><br>
      <%= text_area_tag "comments", nil, placeholder:"Comments", class: "form-control" %><br>
      <%= submit_tag "Submit", class: "btn btn-basecamp" %>
    <% end %>
  </div>
</div>
